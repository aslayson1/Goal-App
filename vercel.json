{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "git": {
    "deploymentEnabled": {
      "main": true,
      "master": true
    }
  },
  "github": {
    "silent": false,
    "autoJobCancellation": false
  },
  "buildCommand": "npm run build",
  "ignoreCommand": "echo 'Force build' && exit 1"
}
